{
  "workbench.iconTheme": "material-icon-theme",
  "open-in-browser.default": "firefox",
  "window.zoomLevel": 0,
  "window.titleBarStyle": "custom",
  "editor.minimap.enabled": false,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/**/node_modules/**": true
  },
  "editor.formatOnSave": true,
  "go.formatTool": "gofmt",
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "telemetry.enableCrashReporter": false,
  "vim.useCtrlKeys": false,
  "workbench.panel.defaultLocation": "right",
  "editor.fontFamily": "Fira Code Retina",
  "editor.fontLigatures": true,
  // "terminal.integrated.fontFamily": "Fira Code Retina",
  "turboConsoleLog.logMessagePrefix": "LOG",
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "[Dd]ockerfile*": "dockerfile",
    "docker-compose*": "yaml"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "latex-workshop.view.pdf.viewer": "tab",
  "python.jediEnabled": false,
  "keyboard.dispatch": "keyCode",
  "oneDarkPro.editorTheme": "solarized",
  "oneDarkPro.vivid": true,
  "vs-kubernetes": {
    "vs-kubernetes.minikube-path": "/home/brian/.vs-kubernetes/tools/minikube/linux-amd64/minikube",
    "vs-kubernetes.minikube-path.linux": "/home/brian/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true,
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "cloudcode.gke": {
    "projectIds": [
      "agilis-corporate"
    ]
  },
  "editor.fontSize": 15,
  "python.terminal.activateEnvironment": false,
  "terminal.integrated.scrollback": 3000,
  "workbench.colorTheme": "One Dark Pro",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}